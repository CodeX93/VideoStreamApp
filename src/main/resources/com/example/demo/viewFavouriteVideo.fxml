   <children>
      <ListView fx:id="videoList" fixedCellSize="30.0" layoutX="14.0" layoutY="109.0" onMouseClicked="#viewVideo" prefHeight="277.0" prefWidth="568.0" />
      <Label alignment="CENTER" layoutX="7.0" layoutY="14.0" prefHeight="17.0" prefWidth="603.0" text="My Favourite Videos">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="backToHome" layoutX="14.0" layoutY="77.0" mnemonicParsing="false" onAction="#backToHome" prefHeight="25.0" prefWidth="65.0" text="Back" />
   </children>
</AnchorPane>
